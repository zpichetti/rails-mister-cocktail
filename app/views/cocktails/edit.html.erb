<div class="container">
  <div class="col-sm-4 col-sm-offset-4">
    <h1>Add cocktail</h1>
    <%= simple_form_for(@cocktail) do |f| %>
        <%= f.input :name %>
        <%= f.input :description %>
        <%= f.input :image_url %>
        <%= f.submit "add a review", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>